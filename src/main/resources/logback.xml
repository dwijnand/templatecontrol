
<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%level %logger{15} - %message%n%xException{10}</pattern>
    </encoder>
  </appender>

  <logger name="templatecontrol" level="DEBUG"/>

  <root level="INFO">
    <appender-ref ref="STDOUT" />
  </root>

</configuration>
